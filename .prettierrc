{
  "arrowParens": "always",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": false,
  "useTabs": false,
  "jsxSingleQuote": false,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "importOrder": [
    "^node:(.*)$",
    "^(react)(/(.*))?$",
    "^(next)(/(.*))?$",
    "<THIRD_PARTY_MODULES>",
    "^@/stuff/(.*)$",
    "^@/api/(.*)$",
    "^@/stores/(.*)$",
    "^@/hooks/(.*)$",
    "^@/(features|utils)/(.*)$",
    "^@/(layouts|components)/(.*)$",
    "^@/pages/(.*)$",
    "^@/types/(.*)$",
    "^@/images/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
